[color]
	ui = true
[color "branch"]
  	current = yellow reverse
  	local = yellow
  	remote = green
[color "diff"]
  	meta = yellow bold
  	frag = magenta bold
  	old = red
  	new = green
[init]
	defaultBranch = main
[core]
	excludesfile = ~/.dotfiles/git/.gitignore_global
[credential]
	helper = osxkeychain